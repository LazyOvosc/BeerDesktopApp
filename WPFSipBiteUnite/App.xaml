             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WPFSipBiteUnite"
             StartupUri="MainView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="UIColors.xaml" />
                <ResourceDictionary Source="CustomButtons.xaml" />
                <ResourceDictionary Source="CustomInput.xaml" />
                <ResourceDictionary Source="CustomNavButtons.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
